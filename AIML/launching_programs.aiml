<aiml version="1.0.1" encoding="UTF-8">

 <!-- Launching programs -->

     <category>
         <pattern>OPEN *</pattern>
         <template>
             <random>
                 <li>Here you go!</li>
                 <li>Opening <star/></li>
             </random>
             <system>bash -c "<star/> &amp;> /dev/null &amp;"</system>
         </template>
     </category>

    <category>
