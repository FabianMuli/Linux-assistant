            </random>
            <system>bash -c "thunderbird &amp;> /dev/null &amp;"</system>
        </template>
    </category>

    <category>
        <pattern>CHECK MESSAGES</pattern>
        <template>
            <srai>CHECK EMAIL</srai>
        </template>
    </category>

